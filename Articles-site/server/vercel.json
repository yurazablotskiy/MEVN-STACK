{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
   {
      "src": "/get-articles",
      "dest": "/index.js"
    },
    {
      "src": "/get-posts",
      "dest": "/index.js"
    },
    {
      "src": "/add-post",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
