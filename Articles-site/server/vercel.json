{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "api/*": {
      "cors": {
        "origin": "https://article-site-server.vercel.app",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "credentials": true
      }
    }
  }
}
