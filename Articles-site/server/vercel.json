{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "OPTIONS, GET, POST",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept"
      }
    }
  ],
  "builds": [{ "src": "/index.js", "use": "@now/node" }]
}

